<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.example.spring95.domain.dto.UserDto" alias="userDto"/>
		<typeAlias type="com.example.spring95.domain.dto.BoardDto" alias="boardDto"/>
		<typeAlias type="com.example.spring95.domain.dto.FileDto" alias="fileDto"/>
		<typeAlias type="com.example.spring95.domain.dto.CommentDto" alias="commentDto"/>
		<typeAlias type="com.example.spring95.domain.dto.ItemDto" alias="itemDto"/>
	</typeAliases>
</configuration>